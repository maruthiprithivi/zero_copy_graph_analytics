        {
          "label": "OWNS_ACCOUNT",
          "mappedTableSource": {
            "catalog": "clickhouse_local",
            "schema": "default",
            "table": "fraud_accounts",
            "metaFields": {
              "id": "account_id",
              "from": "customer_id",
              "to": "account_id"
            }
          }
        },
        {
          "label": "USED_DEVICE",
          "mappedTableSource": {
            "catalog": "clickhouse_local",
            "schema": "default",
