            "schema": "default",
            "table": "fraud_accounts",
            "metaFields": {
              "id": "account_id"
            }
          }
        },
        {
          "label": "FraudDevice",
          "mappedTableSource": {
            "catalog": "clickhouse_local",
            "schema": "default",
            "table": "fraud_devices",
            "metaFields": {
              "id": "device_id"
            }
          }
        },
        {
          "label": "FraudMerchant",
          "mappedTableSource": {
            "catalog": "clickhouse_local",
            "schema": "default",
            "table": "fraud_merchants",
            "metaFields": {
              "id": "merchant_id"
            }
          }
        }
      ],
      "edges": [
        {
          "label": "FRAUD_TRANSACTION",
          "mappedTableSource": {
            "catalog": "clickhouse_local",
            "schema": "default",
